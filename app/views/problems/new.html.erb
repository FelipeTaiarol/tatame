<h2>New Problem</h2>
<% form_for @problem do |f| %>
  <%= f.label :title %><br/>
  <%= f.text_field :title %><br/>
  <%= f.label :kata %><br/>
  <%= f.check_box :kata %><br/>
  <br/>
  <%= f.label :description %><br/>
  <%= f.text_area :description, :rows => 10 %><br/>
  <br/>
  <%= f.submit 'Add' %>
<% end %>

<%= link_to 'Back', problems_path %>
