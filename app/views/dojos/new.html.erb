<% form_for :dojo, @dojo, :url => { :action => "create" } do |f| %>
	<%= label(:dojo, :date) %>
	<%= date_select ("dojo", "date") %>
	
	<%= label(:dojo, :time) %>
	<%= time_select ("dojo", "time") %>
	
	<%= label(:dojo, :comment) %>
	<%= f.text_area :comment %>

	<%= submit_tag 'Create' %>
<% end %>
