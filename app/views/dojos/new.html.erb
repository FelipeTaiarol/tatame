<%= error_messages_for :dojo %>

<% form_for :dojo, @dojo, :url => { :action => "create" } do |f| %>
	<%= label(:dojo, :date) %>
	<%= datetime_select("dojo", "date", :minute_step => 15) %>
	
	<%= label(:dojo, :comment) %>
	<%= f.text_area :comment %>

	<%= submit_tag 'Create' %>
<% end %>

<%= link_to 'Back', root_path %>